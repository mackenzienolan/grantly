{
    "$schema": "https://turbo.build/schema.json",
    "ui": "stream",
    "tasks": {
      "//#sso": {
        "cache": false
      },
      "build": {
        "dependsOn": ["^build"],
        "inputs": ["$TURBO_DEFAULT$", ".env*"],
        "outputs": [".next/**", "!.next/cache/**"]
      },
      "lint": {
        "dependsOn": ["^lint"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "typecheck": {},
      "clean": {},
      "test": {}
    }
  }
  